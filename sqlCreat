CREATE SCHEMA `crawler` ;
CREATE TABLE `crawler`.`crawler4S` (
  `id` INT(11) NOT NULL AUTO_INCREMENT COMMENT '',
  `name` VARCHAR(200) NULL COMMENT '',
  `phone` VARCHAR(200) NULL COMMENT '',
  PRIMARY KEY (`id`)  COMMENT '');
ALTER TABLE `crawler`.`crawler4S`
CHANGE COLUMN `name` `name` VARCHAR(200) CHARACTER SET 'utf8' COLLATE 'utf8_general_ci' NULL DEFAULT NULL COMMENT '' ;